{
  "name": "@fecusio/core-js",
  "version": "1.3.0",
  "description": "Fecusio Core API SDK",
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.cjs"
    }
  },
  "files": [
    "dist/**"
  ],
  "scripts": {
    "clean": "rimraf dist node_modules",
    "prebuild": "npm run clean && npm ci",
    "build": "npm run build:types && npm run build:es && npm run build:cjs",
    "build:types": "tsc --project .",
    "build:es": "babel ./src --ignore='**/*.test.ts' --config-file=./babel.config.json --extensions=.ts,.cts --source-root=./src --out-dir=./dist",
    "build:cjs": "babel ./src --ignore='**/*.test.ts' --config-file=./babel.config.cjs.json --extensions=.ts,.cts --source-root=./src --out-dir=./dist --out-file-extension .cjs",
    "prerelease": "npm run build",
    "release": "np",
    "test": "exit 0",
    "format": "prettier --write src/",
    "format:check": "prettier --check src/",
    "lint": "eslint src/ --fix"
  },
  "keywords": [
    "feature-flags",
    "feature-toggles",
    "fecusio",
    "sdk",
    "api"
  ],
  "author": "Fecusio",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.0.0",
    "typescript": "^5.0.0",
    "rimraf": "^6.0.1",
    "@babel/cli": "^7.19.3",
    "@babel/core": "^7.7.2",
    "@babel/plugin-proposal-export-namespace-from": "^7.18.9",
    "@babel/preset-env": "^7.7.1",
    "@babel/preset-typescript": "^7.18.6",
    "babel-plugin-add-import-extension": "^1.6.0",
    "babel-plugin-transform-inline-environment-variables": "^0.4.4",
    "prettier": "^3.4.2",
    "eslint": "^9.17.0",
    "eslint-config-prettier": "^10.0.1",
    "@eslint/js": "^9.19.0",
    "typescript-eslint": "^8.23.0",
    "np": "^10.2.0"
  },
  "dependencies": {
    "axios": "^1.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fecusio/core-js.git"
  },
  "bugs": {
    "url": "https://github.com/fecusio/core-js/issues"
  },
  "homepage": "https://github.com/fecusio/core-js#readme",
  "np": {
    "yarn": false
  }
}
